<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.edu.sict.musicplayer">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_mplayer"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="cn.edu.sict.musicplayer.MusicListActivity"/>
        <activity android:name="cn.edu.sict.musicplayer.RegisterActivity"/>
        <activity android:name="cn.edu.sict.musicplayer.HomeActivity" />
        <activity android:name="cn.edu.sict.musicplayer.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>